{
    "version": 2,
    "builds": [
      {
        "src": "Frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "Frontend/dist"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/users/:userId/verify/:token",
        "destination": "/api/verify"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }